{
    "name": "somnambulist/symfony-micro-service",
    "description": "A pre-configured Symfony project designed for micro services with docker files",
    "keywords": ["framework", "symfony", "doctrine", "ddd", "micro service", "php-pm"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "somnambulist/api-bundle": "~3.0",
        "somnambulist/domain": "~4.1",
        "somnambulist/form-request-bundle": "^1.0",
        "somnambulist/read-models-bundle": "^2.0",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "^1.12",
        "symfony/framework-bundle": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/monolog-bundle": "~3.6",
        "symfony/proxy-manager-bridge": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.5",
        "doctrine/doctrine-fixtures-bundle": "~3.4",
        "fakerphp/faker": "~1.13",
        "liip/test-fixtures-bundle": "~1.11",
        "phpunit/phpunit": "~9.5",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/debug-bundle": "5.2.*",
        "symfony/phpunit-bridge": "5.2.*",
        "symfony/stopwatch": "5.2.*",
        "symfony/web-profiler-bundle": "5.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "process-timeout": 3600,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
