{
    "name": "somnambulist/symfony-micro-service",
    "description": "A pre-configured Symfony project designed for micro services with docker files",
    "keywords": ["framework", "symfony", "doctrine", "ddd", "micro service", "php-pm"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "somnambulist/api-bundle": "^3.7",
        "somnambulist/domain": "^4.3",
        "somnambulist/form-request-bundle": "^2.0",
        "somnambulist/read-models-bundle": "^2.0",
        "symfony/amqp-messenger": "6.0.*",
        "symfony/doctrine-messenger": "6.0.*",
        "symfony/redis-messenger": "6.0.*",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/messenger": "^6.0",
        "symfony/monolog-bundle": "~3.7",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/property-access": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "~3.4",
        "fakerphp/faker": "~1.17",
        "liip/test-fixtures-bundle": "~2.2.0",
        "phpunit/phpunit": "~9.5",
        "symfony/browser-kit": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/flex": "^2.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "process-timeout": 3600,
        "bin-dir": "bin",
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
